{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "isat_extract",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build_debug/isat_extract",
            "args": [
                "-i",
                "/home/recon/.insightat/tasks/{8bb958b6-2797-465b-82b9-e116e3b7165d}/image_list.json",
                "-o",
                "/home/recon/.insightat/tasks/{8bb958b6-2797-465b-82b9-e116e3b7165d}/output",
                "-v"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}